{
  "name": "markdown-links",
  "displayName": "Markdown Links",
  "description": "Adds command - Show Graph - that displays a graph of local links between markdown files in the current working directory",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:markdown-links.showGraph"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "markdown-links.showGraph",
        "title": "Show Graph"
      }
    ],
    "configuration": {
      "title": "Markdown Links",
      "properties": {
        "markdown-links.column": {
          "type": "string",
          "default": "one",
          "description": "- active – in the currently focused column.\n- beside – other than the current.\n- one (default), two, three, four, five, six, seven, eight, nine – respective editor columns."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.45.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "license": "MIT",
  "dependencies": {
    "@types/md5": "^2.2.0",
    "d3": "4",
    "md5": "^2.2.1",
    "remark-parse": "^8.0.2",
    "unified": "^9.0.0"
  }
}
